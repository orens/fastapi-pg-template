    "version": "0.2.0",
    "configurations": [
        {
            "name": "Click - main.py",
            "type": "debugpy",
            "request": "launch",
            "python": "${workspaceFolder}/.venv/bin/python",
            "module": "main",
            // "args": ["say-hello", "Sir"],
            // "args": ["use-submodule"],
            "args": ["add", "-a", "530", "--second", "1454"],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal"
        },
        {
            "name": "server.py",
            "type": "debugpy",
            "request": "launch",
            "python": "${workspaceFolder}/.venv/bin/python",
            "module": "fastapi",
            "args": ["dev", "server.py"],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal"
        },
        {
            "name": "server_with_db.py",
            "type": "debugpy",
            "request": "launch",
            "python": "${workspaceFolder}/.venv/bin/python",
            "module": "fastapi",
            "args": ["dev", "server_with_db.py"],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal"
        }
    ]
}